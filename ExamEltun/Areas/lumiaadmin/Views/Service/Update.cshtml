@model Settings
<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Employee form</h4>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for=Key for="exampleInputUsername1">Key</label>
                    <input asp-for="Key" class="form-control" id="exampleInputUsername1" placeholder="Username" readonly>
                    <span asp-validation-for="Key" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for=Value for="exampleInputUsername1">Value</label>
                    <input asp-for="Value" class="form-control" id="exampleInputUsername1" placeholder="Value">
                    <span asp-validation-for="Value" class="text-danger"></span>
                </div>
              
             
                <button type="submit" class="btn btn-primary me-2">Update</button>
                <button class="btn btn-light">Cancel</button>
            </form>
        </div>
    </div>
</div>