@model List<Settings>
@{
    int count = 1;
}
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Employee Table</h4>
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>
                                No
                            </th>
                       
                            <th>
                                Key
                            </th>
                          
                            <th>
                                Value
                            </th>
                            <th> Settings</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @(count++)
                                </td>
                             
                                <td>
                                   @item.Key
                                </td>
                                <td>
                                    @item.Value
                                </td>
                                <td>
                                    <a class="btn btn-warning" asp-action="update" asp-controller="Service" asp-route-id="@item.Id"><i class="mdi mdi-pencil"></i></a>

                                </td>
                                
                            
                            </tr>


                        }
                    </tbody>
                </table>
                <div>
                </div>
            </div>
        </div>
    </div>
</div>